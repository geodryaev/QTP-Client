<Window x:Class="QTP_Client.SQLConnect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QTP_Client"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Title="QTP Client" Height="150" Width="500">
    <Grid Background="#FF9A9A9A">
        <TextBox x:Name="tb_sqlPath" HorizontalAlignment="Left" Margin="25,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="287"/>
        <Button x:Name="b_go" Content="Применить" HorizontalAlignment="Left" Margin="345,0,0,0" VerticalAlignment="Center" Width="133" Click="b_go_Click"/>

    </Grid>
</Window>
